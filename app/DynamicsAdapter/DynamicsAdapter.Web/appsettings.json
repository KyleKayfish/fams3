{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Splunk" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error",
        "DynamicsAdapter.Web.Health.DynamicsHealthCheck": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {SearchRequestKey} {DataPartner}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "Dynadapter"
    }
  },
  "OAuth": {
    "ResourceUrl": "",
    "ClientId": "",
    "Secret": "",
    "OAuthUrl": "",
    "Username": "",
    "Password": ""
  },
  "ApiGateway": {
    "BasePath": ""
  },
  "Scheduler": {
    "Cron": "0/15 * * * * ?"
  },
  "SearchApi": {
    "BaseUrl": "http://localhost:5050"
  },
  "OptionsSet": {
    "BaseUrl": "http://localhost:5050"
  },
  "Redis": {
    "Host": "localhost",
    "Port": 6379,
    "Password": ""
  }
}
