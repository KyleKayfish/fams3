{
  "AllowedHosts": "*",
  "DeepSearch": {
    "MaxWaveCount": 4
  },
  "RabbitMq": {
    "Host": "localhost",
    "Port": 5672,
    "Username": "guest",
    "Password": "guest"
  },
  "Redis": {
    "Password": "",
    "AbortOnConnectFail": false,
    "SyncTimeout": 5000,
    "ConnectTimeout": 10000,
    "ConnectRetry": 4,
    "Hosts": [
      {
        "Host": "localhost",
        "Port": "6379"
      }
    ]
  },
  "SearchApi": {
    "WebHooks": [
      {
        "Name": "DynAdapter",
        "Uri": "http://localhost:9000/PersonSearch"
      }
    ],
    "Timeout": "30",
    "ApiKeyForDynadaptor": "6f975845-91a7-4038-830a-eb222b2559fe"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Splunk" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {SearchRequestKey}{DataPartner}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "SearchApi"
    }
  },
